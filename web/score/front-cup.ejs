    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.1.1/socket.io.js" charset="utf-8"></script>
    <script type="text/javascript">
          // var socket = io.connect('http://localhost:7777');
          var socket = io.connect('http://<%= config.host %>:<%= config.port %>');
          socket.on('front-score-cup', function(data){
            console.log(data)
            if(data.action === "activate"){
              document.querySelector(".score-container").classList.add('active')
            } else if(data.action === "deactivate"){
              document.querySelector(".score-container").classList.add('deactive')
            } else if(data.action === "change"){
              document.querySelector(".teamOne .name-text").innerHTML = data.values.teamOne.name
              document.querySelector(".teamOne .points-text").innerHTML = data.values.teamOne.score
              document.querySelector(".teamTwo .name-text").innerHTML = data.values.teamTwo.name
              document.querySelector(".teamTwo .points-text").innerHTML = data.values.teamTwo.score
            } else if(data.action === "reset"){
              document.querySelector(".score-container").classList.remove('active')
              document.querySelector(".score-container").classList.remove('deactive')
            }
          })

    </script>
